{
    "name": "novam/laravel-core",
    "description": "",
    "authors": [
        {
            "name": "Hilkiah Makemo",
            "email": "hilmak01@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^7.0|^8.0"
    },
    "suggest": {
        "nwidart/laravel-modules": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core\\Boot\\Providers\\CoreServiceProvider",
                "Core\\Boot\\Providers\\MacroServiceProvider"
            ],
            "aliases": {
                "Core": "Core\\Mock\\Facades"
            }
        },
        "installer-name": "laravel-core"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "src/"
        },
        "files": [
            "./lib/helpers/load.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
